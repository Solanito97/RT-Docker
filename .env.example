# Variables de entorno para Request Tracker
# Copia este archivo como .env y modifica según tus necesidades

# Configuración de MySQL
MYSQL_ROOT_PASSWORD=rt_root_password_secure
MYSQL_DATABASE=rt6
MYSQL_USER=rt_user
MYSQL_PASSWORD=rt_password_secure

# Configuración de RT
RT_DATABASE_HOST=mysql
RT_DATABASE_NAME=rt6
RT_DATABASE_USER=rt_user
RT_DATABASE_PASSWORD=rt_password_secure
RT_WEB_DOMAIN=localhost
RT_WEB_PORT=80

# Zona horaria
TZ=America/Mexico_City